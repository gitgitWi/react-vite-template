    "@types/react": "^18.0.33",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "next": "^13.3.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.61.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.2",
    "zustand": "^4.3.7"
  }
